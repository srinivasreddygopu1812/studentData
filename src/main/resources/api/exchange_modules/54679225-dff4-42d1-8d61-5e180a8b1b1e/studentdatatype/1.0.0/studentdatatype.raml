#%RAML 1.0 DataType
type: object
properties:
  number: 
    type: integer
    minimum: 1
    maximum: 60
    required: false
  name:
    type: string
    minLength: 3
    maxLength: 20
  address:
    type: string
    minLength: 3
    maxLength: 30
  class:
    type: string
    example:
      6th
  age:
    type: integer
    minimum: 3
    maximum: 20
  gender:
    type: string
    enum:
      - Male
      - Female